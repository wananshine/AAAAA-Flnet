}
.order-panel .order-inner .order-show .order-card .card-body .form-horizontal .form-addr {
  width: 40%;
}
.order-panel .order-inner .order-show .order-card .card-body .form-horizontal .form-btn {
  margin: 42px auto 0px;
}
.order-panel .order-inner .order-show .order-card .card-body .form-horizontal .form-btn button {

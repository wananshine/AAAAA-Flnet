[v-cloak] {
  display: none;
}
.order-panel {
  width: 100%;
  position: relative;
  overflow: hidden;
}
.order-panel .order-inner {
  width: 1200px;
  margin: auto;
}
.order-panel .order-inner .logo-img {
  height: 60px;
